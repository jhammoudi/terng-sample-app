{
  "name": "tern-starter-kit",
  "version": "1.0.0",
  "description": "A starter web app, built using Typescript, Express js, React js, Node js",
  "scripts": {
    "start": "node build/index.js",
    "start-dev": "concurrently -k npm:start-dev:*",
    "start-dev:ui": "npm start --prefix frontend",
    "start-dev:server": "nodemon backend/index.ts",
    "prod": "npm run build && npm start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rimraf build && mkdir build && cd frontend && npm run build && cd .. && tsc"
  },
  "author": "Jihad Hammoudi",
  "license": "ISC",
  "dependencies": {
    "apollo-server-core": "^3.8.2",
    "apollo-server-express": "^3.8.2",
    "axios": "^0.27.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.18.1",
    "graphql": "^16.5.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.5",
    "concurrently": "^7.2.1",
    "cpr": "^3.0.1",
    "nodemon": "^2.0.15",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
